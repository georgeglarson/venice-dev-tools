{
  "name": "@venice-dev-tools/core",
  "version": "2025.11.6",
  "description": "Core package for the Venice AI unOfficial SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./errors": {
      "import": {
        "types": "./dist/errors/index.d.mts",
        "default": "./dist/errors/index.mjs"
      },
      "require": {
        "types": "./dist/errors/index.d.ts",
        "default": "./dist/errors/index.cjs"
      }
    },
    "./types": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/types/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/types/index.cjs"
      }
    },
    "./utils": {
      "import": {
        "types": "./dist/utils/index.d.mts",
        "default": "./dist/utils/index.mjs"
      },
      "require": {
        "types": "./dist/utils/index.d.ts",
        "default": "./dist/utils/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "test": "vitest run --exclude '**/__integration__/**'",
    "test:watch": "vitest --exclude '**/__integration__/**'",
    "test:coverage": "vitest run --coverage --exclude '**/__integration__/**'",
    "test:integration": "vitest run src/__integration__",
    "test:integration:watch": "vitest src/__integration__",
    "test:all": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "venice",
    "ai",
    "sdk",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/georgeglarson/venice-dev-tools.git",
    "directory": "venice-ai-sdk/packages/core"
  },
  "bugs": {
    "url": "https://github.com/georgeglarson/venice-dev-tools/issues"
  },
  "homepage": "https://github.com/georgeglarson/venice-dev-tools#readme",
  "author": "Venice AI",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.3",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitest/coverage-v8": "^1.3.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0-alpha.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
