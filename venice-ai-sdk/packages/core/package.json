{
  "name": "@venice-dev-tools/core",
  "version": "2025.11.5",
  "description": "Core package for the Venice AI unOfficial SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --clean",
    "dev": "tsup src/index.ts --format cjs --dts --clean --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "test": "vitest run --exclude '**/__integration__/**'",
    "test:watch": "vitest --exclude '**/__integration__/**'",
    "test:coverage": "vitest run --coverage --exclude '**/__integration__/**'",
    "test:integration": "vitest run src/__integration__",
    "test:integration:watch": "vitest src/__integration__",
    "test:all": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "venice",
    "ai",
    "sdk",
    "api"
  ],
  "author": "Venice AI",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.3",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitest/coverage-v8": "^1.3.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0-alpha.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
