{
  "name": "@venice-ai/web",
  "version": "2.0.0",
  "description": "Web implementation of the Venice AI SDK",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --clean --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "clean": "rimraf dist"
  },
  "keywords": [
    "venice",
    "ai",
    "sdk",
    "api",
    "browser"
  ],
  "author": "Venice AI",
  "license": "MIT",
  "dependencies": {
    "@venice-ai/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}