{
  "name": "venice-dev-tools",
  "version": "2.1.0",
  "private": true,
  "description": "unOfficial SDK for the Venice AI API",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm install && cd packages/core && npm install && cd ../node && npm install && cd ../web && npm install && cd ../../ && npm run build:packages",
    "build:packages": "pnpm -r run build",
    "clean": "pnpm -r run clean",
    "dev": "pnpm -r run dev",
    "lint": "pnpm -r run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "keywords": [
    "venice",
    "ai",
    "sdk",
    "api"
  ],
  "author": "Venice AI",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/node": "^22.13.10",
    "pdfkit": "^0.16.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "packageManager": "pnpm"
}